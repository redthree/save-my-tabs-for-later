{
	"manifest_version": 2,
	"name": "Save my tabs for later",
	"description": "This extension will save all your current tabs for later",
	"version": "1.4",
	"key":"",
	"icons":{
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"browser_action": {
		"default_icon": "img/icon128.png",
		"default_popup": "index.html"
	},
	"background": {
		"scripts": ["main.js"]
	},
	"permissions": [
		"tabs",
		"storage"
	]
}